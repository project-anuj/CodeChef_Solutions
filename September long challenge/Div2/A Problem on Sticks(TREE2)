#include<bits/stdc++.h>
using namespace std;
typedef long long ll;
int main()
{
	int t;
	cin>>t;
	while(t--)
	{
		int n;
		cin>>n;
		ll a[n];
		for(int i=0;i<n;i++)
		{
			cin>>a[i];
		}
		
	    set<int> s(a,a+n);
	    vector<int> v(s.begin(),s.end()) ;
	    int count=0;
	    for(int i=0;i<v.size();i++)
	    {
	        if(v[i]!=0)
	        count++;
	    }
		cout<<count<<endl;
	}
}
